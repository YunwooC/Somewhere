}
  
void loop(void) {

  int value = analogRead(light);

  u8g2.firstPage();
  do {

    draw(value);
    
  } while(u8g2.nextPage());
  
  
  if(digitalRead(button) == HIGH){
     num1 = random(1, 3);
     num2 = random(1, 3);
     num3 = random(1, 3);
     num4 = random(1, 3);
  }

}
