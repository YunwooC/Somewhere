int button = 6;
char light = A6;
int num1;
int num2;
int num3;
int num4;

static void setup(void) {
  u8g2.begin();
  pinMode(button, INPUT);
  Serial.begin(9600);

  num1 = random(1, 3);
  num2 = random(1, 3);
  num3 = random(1, 3);
  num4 = random(1, 3);
}

void draw(int value) {
      if(num1 == 1){
        if(value < 100){
          u8g2.drawXBMP(0,0, Ranch_1_Cloud_width, Ranch_1_Cloud_height, Ranch_1_Cloud_bits);
        }
        else {
          u8g2.drawXBMP(0,0, Ranch_1_Cloud_Day_width, Ranch_1_Cloud_Day_height, Ranch_1_Cloud_Day_bits);
        }
      }
      else if(num1 == 2){
        
        if(value < 100){
          u8g2.drawXBMP(0,0, Ranch_1_Sunny_width, Ranch_1_Sunny_height, Ranch_1_Sunny_bits);
        }
        else {
          u8g2.drawXBMP(0,0, Ranch_1_Sunny_Day_width, Ranch_1_Sunny_Day_height, Ranch_1_Sunny_Day_bits);
        }
      }
      

      if(num2 == 1){
